<section class="container">
  <h3 class="goal-name">{{goal.name}}</h3>
  <h4>Opening Hour: {{format_time goal.starting_date}}</h4>
  <h4>Closing Hour: {{format_time goal.ending_date}}</h4>
  <section class="flex-row justify-center">
    {{#each goal.paintings as |painting|}}
    {{> painting-details}}
    {{/each}}
  </section>
</section>
